extends layout/main.pug


include ../blocks/s-content/_s-content.pug
include ../blocks/s-contact/_s-contact.pug
include ../blocks/s-menu/_s-menu.pug
include ../blocks/s-about/_s-about.pug
include ../blocks/s-advantages/_s-advantages.pug
include ../blocks/s-rews/_s-rews.pug
include ../blocks/s-docs/_s-docs.pug
include ../blocks/s-news/_s-news.pug
include ../blocks/s-blog/_s-blog.pug
include ../blocks/s-logo/_s-logo.pug
include ../blocks/s-cases/_s-cases.pug
include ../blocks/sMap/_sMap.pug
include ../blocks/sOrderWork/_sOrderWork.pug

block variables
	- let  mainPage= null
block title
	- let  title= 'title'
	title= title
block pagecontent 
	+mob-mnu()
	+header()
		+top-line()
		+top-nav()
	+header-block()
	+s-menu
	+s-about
		+s-cases
	+s-advantages
	+sMap
	+sOrderWork
	.container
		.row
			.col-lg-6
				+s-content
			.col-lg-6
				+s-rews
	+s-docs
	+s-form()
	.section-wrap.container
		.row
			.col-lg-6
				+s-news
			.col-lg-6
				+s-blog
	+s-logo
	+footer()